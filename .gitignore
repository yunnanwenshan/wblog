# Clacky specific configuration files
.1024*
!.1024
.breakpoints

# Ignore bundler config.
/.bundle

# Ignore Ruby/Rails specific files
*.rbc
capybara-*.html
.rspec
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/db/*.sqlite3
/db/*.sqlite3-journal
/coverage/
/spec/tmp
.env
.env.*

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore Yarn/Node specific files
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

*.swp

/public/uploads/*
/public/assets/*
/tags

/config/application.yml
/config/mongoid.yml
/config/database.yml
/config/master.key
**.orig
*.old
*.bak
*~
.byebug_history
.DS_Store

/app/assets/builds/*
!/app/assets/builds/.keep